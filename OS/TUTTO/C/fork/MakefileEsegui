
GCC=gcc
CFLAGS=-ansi -Wpedantic -Wall -D_REENTRANT -D_POSIX_C_SOURCE=200112L
LFLAGS=

all: esegui.exe


esegui.exe:	esegui.o
		${GCC} ${CFLAGS} -o esegui.exe esegui.o ${LFLAGS}

esegui.o:	esegui.c printerror.h
		${GCC} ${CFLAGS} -c esegui.c


.PHONY:		clean


clean:	
		rm -f esegui.exe esegui.o  *~


